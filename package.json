{
  "name": "cg-2019-2n-t1",
  "version": "1.0.0",
  "private": true,
  "description": "Trabalho com o objetivo de apresentar um ambiente composto por árvores, céu, profundidade, fonte de luz (sol) e o mais importante, a movimentação de um ou mais Pokémon, a fim de criar um simples cenário.",
  "keywords": [
    "cg",
    "three",
    "pokemon",
    "react",
    "fiber",
    "typescript"
  ],
  "author": "Marcelo Knob",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "gen:gltf": "gltfjsx -t -T -o src/components/model/model.tsx"
  },
  "dependencies": {
    "@react-three/drei": "^9.99.4",
    "@react-three/fiber": "^8.15.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.161.0"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^2.2.2",
    "@tsconfig/vite-react": "^3.0.0",
    "@types/node": "^20.11.22",
    "@types/react": "^18.2.60",
    "@types/react-dom": "^18.2.19",
    "@types/three": "^0.161.2",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "gltfjsx": "^6.2.16",
    "three-stdlib": "^2.29.4",
    "typescript": "^5.3.3",
    "vite": "^5.1.4"
  }
}
